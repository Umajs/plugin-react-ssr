{
  "name": "@umajs/plugin-react-ssr",
  "version": "2.0.4-alpha.0",
  "description": "In umajs, React is used to develop the plug-in of SPA and MPA, which supports server-side rendering and client-side rendering",
  "author": "zunyi_zjj@163.com",
  "license": "MIT",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "fix": "esw src --fix --ext .ts",
    "lint": "npx eslint src --ext .ts",
    "lint-w": "esw src --clear --color -w --ext .ts",
    "build-w": "tsc -w --inlineSourceMap",
    "start": "run-p lint-w build-w",
    "prebuild": "npm run lint",
    "build": "tsc",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "@srejs/react": "^1.3.4-alpha.0",
    "consolidate": "^0.16.0",
    "get-stream": "^6.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.19",
    "@types/node": "^12.12.9",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@umajs/core": "^2.0.1",
    "eslint": "^7.15.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-watch": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Umajs/plugin-react-ssr.git"
  },
  "bugs": {
    "url": "https://github.com/Umajs/plugin-react-ssr"
  },
  "homepage": "https://github.com/Umajs/plugin-react-ssr#readme"
}
